<div id="add_members_tab" class="hidden">
  <%= partial :search_container %>
  <ul class="users">
    <% 4.times do |i| %>
        <li>
          <div>
            <p>John Doe</p>
            <span>john.doe@example.com</span>
          </div>
          <a href="#" class="rounded"><i class="fa fa-plus"></i></a>
        </li>
    <% end %>
    <li class="separator">Nieuw toegevoegd</li>
    <% 2.times do |i| %>
        <li>
          <div>
            <p>Mary Doe</p>
            <span>mary.doe@example.com</span>
          </div>
          <a href="#" class="rounded"><i class="fa fa-minus"></i></a>
          <select name="roles" id="roles">
            <option value="admin">Admin</option>
            <option value="manager" selected>Member</option>
            <option value="member">Manager</option>
          </select>
        </li>
    <% end %>
  </ul>
</div>